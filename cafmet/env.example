# Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# Google Service Account Credentials (JSON format)
# This should be the entire JSON content of your service account key file
GOOGLE_SERVICE_ACCOUNT_CREDENTIALS={"type":"service_account","project_id":"your-project-id","private_key_id":"your-private-key-id","private_key":"-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n","client_email":"your-service-account@your-project.iam.gserviceaccount.com","client_id":"your-client-id","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/your-service-account%40your-project.iam.gserviceaccount.com","universe_domain":"googleapis.com"}

# Google Maps API Key
MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Support Email
SUPPORT_EMAIL=support@yourdomain.com

# Google Sheets Configuration
# These URLs should be reviewed for sensitivity
GOOGLE_SHEETS_CSV_URL=https://docs.google.com/spreadsheets/d/e/YOUR_SHEET_ID/pub?gid=255114807&single=true&output=csv
GOOGLE_SHEETS_ID=YOUR_SHEET_ID_HERE

# Additional Google Sheets URLs for different features
GOOGLE_SHEETS_AUTH_URL=https://docs.google.com/spreadsheets/d/e/YOUR_SHEET_ID/pub?gid=0&single=true&output=csv
GOOGLE_SHEETS_B2B_URL=https://docs.google.com/spreadsheets/d/e/YOUR_SHEET_ID/pub?gid=251336202&single=true&output=csv
GOOGLE_SHEETS_USER_URL=https://docs.google.com/spreadsheets/d/e/YOUR_SHEET_ID/pub?gid=0&single=true&output=csv

# Google Drive PDF URL
GOOGLE_DRIVE_PDF_URL=https://drive.google.com/uc?export=download&id=YOUR_PDF_FILE_ID 